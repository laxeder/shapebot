  "main": "dist/app.js",
  "scripts": {
    "start": "export NODE_ENV=production && node dist/app.js",
    "prod": "export NODE_ENV=production && ts-node -r tsconfig-paths/register app.ts --prod",
    "stage": "export NODE_ENV=production && ts-node -r tsconfig-paths/register app.ts --prod",
    "dev": "export NODE_ENV=development && ts-node-dev -r tsconfig-paths/register app.ts --dev",
    "generate": "export NODE_ENV=development && ts-node-dev -r tsconfig-paths/register generateBot.ts --dev",
    "build": "npm run clean && tsc && tsc-alias",
    "clean": "rimraf dist"
  },
