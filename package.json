    "rimraf": "5.0.1",
    "rompot": "1.6.5",
    "ts-node-dev": "2.0.0",
    "tsc-alias": "1.8.6",
    "tsconfig-paths": "4.2.0",
    "uuid": "9.0.0",
    "winston": "3.9.0"
  }
